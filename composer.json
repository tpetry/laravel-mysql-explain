{
    "name": "tpetry/laravel-mysql-explain",
    "description": "Get MySQL EXPLAIN plans that are understandable for humans",
    "keywords": [
        "laravel",
        "mysql",
        "explain"
    ],
    "homepage": "https://github.com/tpetry/laravel-mysql-explain",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Petry",
            "email": "github@tpetry.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^3.1|^4.0|^5.0|^6.0|^7.0",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tpetry\\MysqlExplain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tpetry\\MysqlExplain\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tpetry\\MysqlExplain\\MysqlExplainServiceProvider"
            ],
            "aliases": {
                "MysqlExplain": "Tpetry\\MysqlExplain\\Facades\\MysqlExplain"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
